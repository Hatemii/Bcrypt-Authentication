<h1>Log In</h1>


<%= form_tag sessions_path do %>
<div class="form-group">
    <%= label_tag :email %><br />
    <%= text_field_tag :email %>
</div>

<div class="form-group">
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
</div>

<button type="submit" class="btn btn-primary">Log In</button>

<% end %>